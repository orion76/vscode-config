{
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        // "*.scad": "javascript",
    },
    "javascript.validate.enable": false,
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.largeFileOptimizations": false,
    "typescript.tsserver.log": "verbose",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#067ad8",
        "tab.unfocusedActiveBorder": "#444444",
    },
    "workbench.editor.wrapTabs": true,
    "workbench.colorTheme": "Default Dark+",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editor.tabSizingFixedMinWidth": 140,
    "window.title": "${dirty}${separator}${activeRepositoryName}",
    "typelens.showReferencesForProperties": false,
    "typelens.showReferencesForMethods": false,
    "files.exclude": {
        "**/.angular": true
    },
    "workbench.editor.tabSizingFixedMaxWidth": 200,
    "editor.minimap.enabled": false,
    "scm.inputFontSize": 14,
    "window.zoomLevel": 1,
    "editor.tokenColorCustomizations": {
       "[Default Dark+]": {
        "keywords": "#327aff",
        "variables": "#fed19e",
        "functions": "#FFFFFF",
        
       } 
    },
    "workbench.editor.labelFormat": "medium",
}